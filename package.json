{
  "name": "ytclipper",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn build && node app.js",
    "build": "yarn build:public && yarn build:css && yarn build:videojs",
    "build:public": "copyfiles -f ui/* public/",
    "build:css": "postcss ui/styles/tailwind.css -o public/styles/tailwind.css && postcss ui/styles/custom.css -o public/styles/custom.css",
    "build:videojs": "copyfiles -f node_modules/videojs-youtube/dist/Youtube.min.js public/scripts/ && copyfiles -f node_modules/video.js/dist/video-js.min.css public/styles/"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "child_process": "^1.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "videojs-youtube": "^3.0.1",
    "yarn": "^1.22.19",
    "youtube-dl-exec": "~2.4.7"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "tailwindcss": "^2.2.4"
  }
}
