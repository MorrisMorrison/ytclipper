  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "child_process": "^1.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "^0.0.1-security",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "yarn": "^1.22.19",
    "youtube-dl": "~3.4.1"
  },
  "devDependencies": {
    "tailwindcss": "^2.2.4",
    "copyfiles":"^2.4.1"
  }
}
